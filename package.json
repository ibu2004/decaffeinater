{
  "name": "decaffeinater",
  "description": "An app which closes apps if you use them too much.",
  "version": "1.2.0",
  "main": "app/main.js",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.62.2"
  },
  "scripts": {
    "build": "webpack --progress",
    "watch": "webpack -w --progress",
    "no-build": "electron .",
    "start": "webpack --progress && electron .",
    "pack": "webpack --progress && build --dir",
    "dist": "webpack --progress && build"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "electron": "^1.4.12",
    "electron-builder": "^10.8.1",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "json-loader": "^0.5.4",
    "webpack": "2.2.0-rc.1"
  },
  "build": {
    "appId": "com.decaffeinater",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "linux": {
      "target": "deb",
      "synopsis": "An app which closes apps if you use them too much.",
      "category": "Utility",
      "desktop": "build/decaffeinater.desktop"
    }
  }
}
