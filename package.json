{
  "name": "decaffeinater",
  "description": "An app which closes apps if you use them too much.",
  "version": "1.3.0-rc.1",
  "main": "app/main.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ibujs/decaffeinater.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "concurrently": "^3.4.0",
    "material-ui": "1.0.0-alpha.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "semantic-ui-react": "^0.68.4"
  },
  "scripts": {
    "build": "webpack --progress",
    "watch": "webpack -w --progress",
    "start": "npm run build && concurrently \"npm run watch\" \"electron .\"",
    "nobuild": "electron . --inspect=9222 --remote-debugging-port=9229",
    "dist": "npm run build && build",
    "pack": "npm run build && build --dir",
    "make-exe": "npm run dist && node build/build.js",
    "test": "mocha --require babel-register"
  },
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/chai": "^3.5.2",
    "@types/electron": "^1.4.38",
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "@types/webpack": "^2.2.15",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-babili": "^0.1.2",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "devtron": "^1.4.0",
    "electron": "^1.6.10",
    "electron-builder": "^13.9.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^8.7.0",
    "electron-winstaller": "^2.5.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "mocha": "^3.4.2",
    "webpack": "^2.6.1"
  },
  "build": {
    "appId": "com.decaffeinater",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "linux": {
      "target": "deb",
      "synopsis": "An app which closes apps if you use them too much.",
      "category": "Utility",
      "desktop": "build/decaffeinater.desktop"
    }
  },
  "optionalDependencies": {
    "electron-installer-debian": "^0.5.1",
    "electron-installer-redhat": "^0.5.0"
  }
}
