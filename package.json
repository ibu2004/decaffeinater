{
  "name": "decaffeinater",
  "productName": "decaffeinater",
  "version": "1.2.0",
  "description": "An app which closes apps if you use them too much.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "lint": "eslint src",
    "update-semui-unix": "cp -f node_modules/semantic-ui-css/semantic.min.css src/semantic.min.css",
    "update-semui-win": "robocopy node_modules\\semantic-ui-css\\semantic.min.css src\\semantic.min.css"
  },
  "keywords": [],
  "author": "ibrahim",
  "license": "Apache-2.0",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "decaf"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "decaf"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "electron-compile": "^5.1.2",
    "eslint-config-airbnb": "^13.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.63.5"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-env": "^1.1.6",
    "babel-preset-react": "^6.16.0",
    "electron-prebuilt-compile": "1.4.13",
    "eslint": "^3.13.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0"
  }
}
